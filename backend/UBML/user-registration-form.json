{
  "id": "user-registration-form",
  "type": "response-form",
  "appearance": {
    "class": "user-registration-form"
  },
  "contents": [
    {
      "id": "username-input",
      "type": "input-group",
      "label": "用户名",
      "placeholder": "请输入用户名",
      "required": true,
      "maxLength": 20,
      "inputType": "input",
      "visible": true,
      "editable": true
    },
    {
      "id": "password-input",
      "type": "input-group",
      "label": "密码",
      "placeholder": "请输入密码",
      "required": true,
      "inputType": "password",
      "enableViewPassword": true,
      "visible": true,
      "editable": true
    },
    {
      "id": "phone-input",
      "type": "input-group",
      "label": "手机号",
      "placeholder": "请输入手机号",
      "required": true,
      "maxLength": 11,
      "visible": true,
      "editable": true
    },
    {
      "id": "register-button",
      "type": "button",
      "text": "注册",
      "textAlign": "middle",
      "onClick": "handleUserRegistration",
      "visible": true
    }
  ],
  "visible": true,
  "labelAutoOverflow": false
}